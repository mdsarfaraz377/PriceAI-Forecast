{
  "version": 2,
  "builds": [
    {
      "src": "price_forecast/price_forecast/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "price_forecast/price_forecast/wsgi.py"
    }
  ]
}
